doctype html
html
  head
    title= 'Jaco van Cranenburgh'
    link(rel='stylesheet', href='/stylesheets/style.css')

  #layout
    #outer
      #static
        .sidemenu
          a(href='/home')
            .item.start#Home Home
            
          a(href='/Education')  
            .item#Education Education
           
          a(href='/Experience')
            .item#Experience Experience

          a(href='/Projects') 
            .item#Projects Projects
          
          a(href='/Papers')
            .item#Papers Papers
            
          a(href='/Contact')  
           .item.end#Contact Contact
         

      #dynamic
        .contentheader
          .title #{title}
        .content     
            body
              block content

script(src="https://code.jquery.com/jquery-3.3.1.min.js")

script.
  window.onload = function(){
    console.log("#{title}")
    var button = document.getElementById("#{title}");
    button.id = "clicked";
    console.log(button.id)
  };




